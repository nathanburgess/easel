        "uuid"                  : "^3.1.0"
    },
    "devDependencies": {
        "babel-cli"          : "^6.26.0",
        "babel-preset-env"   : "^1.6.1",
        "babel-preset-es2017": "^6.24.1",
        "chai"               : "^4.1.2",
        "mocha"              : "^4.1.0"
    },
    "engines"        : {
        "node": ">=9.2.0"
    }
