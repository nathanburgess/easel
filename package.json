{
    "name"           : "easel",
    "version"        : "0.0.2",
    "description"    : "A graphics library built around node-canvas to make creation and editing of procedural images easier.",
    "author"         : "Nathan Burgess",
    "license"        : "MIT",
    "main"           : "dist/Easel.js",
    "repository"     : {
        "type": "git",
        "url" : "github:nathanburgess/easel"
    },
    "scripts"        : {
        "build": "echo Please run 'babel **/** --out-dir dist' on your own",
        "watch": "nodemon --experimental-modules index"
    },
    "keywords"       : [
        "easel",
        "canvas",
